{
  "name": "react-doc-creator",
  "version": "0.0.8",
  "description": "Create documentation for your React projects in a easy way using regular PropTypes declarations.",
  "main": "lib/index.js",
  "bin": {
    "react-doc-creator": "lib/index.js"
  },
  "scripts": {
    "start": "lib/index.js",
    "test": "jest test/*.test.js",
    "build:doc": "./lib/index.js examples --out examples/REACT-PROPTYPES.md --title 'React PropTypes Example'",
    "lint": "eslint lib"
  },
  "author": {
    "name": "Octavio Coria",
    "email": "hi@octaviocoria.com"
  },
  "repository": "github:crearlink/react-doc-creator",
  "license": "MIT",
  "keywords": [
    "react",
    "markdown",
    "documentation",
    "generator",
    "docgen",
    "list of components",
    "components"
  ],
  "dependencies": {
    "cli-table": "0.3.1",
    "colors": "1.2.1",
    "commander": "2.14.1",
    "handlebars": "4.0.11",
    "node-dir": "0.1.17",
    "react-docgen": "2.20.1"
  },
  "devDependencies": {
    "eslint": "4.19.0",
    "jest-cli": "23.0.0-alpha.0",
    "react": "16.2.0"
  }
}
